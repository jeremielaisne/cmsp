<!DOCTYPE html>
<html>
    <head>
        <!--begin::Head-->
        <meta charset="UTF-8">
        <title>{% block title %}CMSP{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% if page is defined %}
            {{ encore_entry_link_tags(page) }}
        {% endif %}
        <meta name="description" content="CMS Personnel, modification de contenus, bonne pratique SEO, gestion de documents" />
        <meta name="keywords" content="Personnel, modification de contenus, bonne pratique SEO, gestion de documents" />
        {# <link rel="canonical" href="Https://preview.keenthemes.com/rider-free" /> #}
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <link rel="shortcut icon" href="/assets/media/logos/favicon.ico" />
        <!--begin::Fonts-->
        {# <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,600,700" /> #}
        <!--end::Fonts-->
        {% block stylesheets %}{% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}

            <!--begin::Javascript-->
            <!--begin::Global Javascript Bundle(used by all pages)-->
            <script src="/assets/plugins/global/plugins.bundle.js"></script>
            <script src="/assets/js/scripts.bundle.js"></script>
            <!--end::Global Javascript Bundle-->
            <!--begin::Page Custom Javascript(used by this page)-->
            <script src="/assets/js/custom/widgets.js"></script>

			<!-- Our project just needs Font Awesome solid + brand -->
            <script defer src="/assets/fontawesome/js/brands.js"></script>
            <script defer src="/assets/fontawesome/js/solid.js"></script>
            <script defer src="/assets/fontawesome/js/fontawesome.min.js"></script>
            <!--end::Page Custom Javascript-->
            <!--end::Javascript-->
        {% endblock %}
    </head>
    <body id="kt_body" class="header-fixed header-tablet-and-mobile-fixed aside-fixed">
        <!--end::Head-->
	    <!--begin::Body-->
        {% block body %}
		
        {% endblock %}
        <!--end::Body-->
        {% block scripts %}
        {% endblock %}
    </body>
</html>
